div#userpanel
	img#icon[src=images/nestor16.png]
	span#userName
		"{{ userName }}"
	a#disconnect[href=#]
		"{{ lang.logout }}"
		
div#main
	ul#pagetree
		@each pages
			li
				a[href=#][data-page={{ id }}]
					"{{ title }}"
			
	div#viewport
		@each pages
			div.pageViewport[data-plugin={{ plugin }}][data-page={{ id }}]
